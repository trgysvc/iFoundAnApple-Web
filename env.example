# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Gemini AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key

# Backend API Configuration
VITE_BACKEND_API_URL=http://localhost:3000/v1
# Production: VITE_BACKEND_API_URL=https://api.ifoundanapple.com/v1

# PAYNET Payment Gateway Configuration
# PAYNET credentials are managed by backend, no frontend configuration needed

# Application Configuration
NODE_ENV=production
PORT=3001
VITE_APP_URL=https://yourdomain.com
VITE_API_URL=https://yourdomain.com

# Security Configuration
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Database Configuration (if using external DB)
DATABASE_URL=your_database_url

# Email Configuration (for notifications)
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password

# Cargo Integration
CARGO_API_KEY=your_cargo_api_key
CARGO_API_URL=https://api.cargo.com

# IBAN Validation
IBAN_VALIDATION_API_KEY=your_iban_validation_api_key
# IBAN_VALIDATION_API_URL=https://api.iban.com/v1/validate (optional, if using external service)

# Audit and Logging
LOG_LEVEL=info
AUDIT_LOG_ENABLED=true
